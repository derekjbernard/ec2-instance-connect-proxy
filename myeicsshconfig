Host eic-dr-vlmap-3
  HostName i-004cb95e71985a510
  User ec2-user
  ProxyCommand eicproxy %r@%h:%p

Host eic-pj-dr-vlamp-4
  HostName i-074207dcd98a3cbb8
  User ec2-user
  ProxyCommand eicproxy %r@%h:%p --use-private-ip --jumphost eic-dr-vlmap-3

  # hack to use tagName. used with command like: `ssh ec2-user@awstn-my-ec2-instance-name`
Host awstn-*
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  ProxyCommand bash -c 'tagname="%h"; eicproxy %r@${tagname#awstn-}:%p --use-tag-name'

Host i-*
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  ProxyCommand eicproxy %r@%h:%p